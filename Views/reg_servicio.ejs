    <form action="/registroservi" method="post">
        <div class="container">
            <label for="">Nombre Servicio:</label> 
            <input type="text" id="nom" name="nom_ser" class="form-control">
            <input type="hidden" id="doc" name="doc"  value="<%=documento%>" class="form-control" readonly>
            <p>
                <label for="">Categoría:</label>
                <select name="categoria" class="form-control">
                    <option value="Salud">Salud</option>
                    <option value="Tecnologia">tecnología</option>
                    <option value="Hogar">Hogar</option>
                    <option value="Mascotas">Mascotas</option>
                </select>
            </p>
            <label for="">Descripción:</label>
            <input type="text" id="des" name="des" class="form-control">
            <label for="">Valor del Servicio:</label>
            <input type="number" id="val" name="val" class="form-control">
            <input type="submit" value="Registrar" id="btn" class="btn btn-success">
                        
        </div>
       </form>
