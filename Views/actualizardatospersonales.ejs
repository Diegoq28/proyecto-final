    <form action="/actualizardatos" method="post">
        <h2>Mis Datos</h2>
        <label for="">Documento:</label>
        <input type="text" id="doc" name="user_doc" value="<%=documento%>" class="form-control" readonly >
        <label for="">Nombre:</label>
        <input type="text" id="nom" name="user_name" value="<%=nombre%>" class="form-control">
