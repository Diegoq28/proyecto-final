        <label for="">Telefono:</label>
        <input type="text" id="tel" name="user_tel" value="<%=telefono%>" class="form-control">
        <label for="">Direccion:</label>
        <input type="text" id="dic" name="user_direc" value="<%=direccion%>" class="form-control">
